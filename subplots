import numpy as np
import matplotlib.pyplot as plt

x= np.arange(0,9,0.1)

y_sin=np.sin(x)

y_cos=np.cos(x)


plt.subplot(2,1,1)
plt.title("TEAM A")
plt.plot(x,y_sin,"m")
plt.show()


plt.subplot(2,1,3)
plt.plot(x,y_cos,"r")
plt.title("TEAM B")

plt.subplot(2,1,2)
plt.plot(x,y_cos,"b")
plt.title("TEAM c")
plt.subplots_adjust(hspace=0.9)                   #hspace helps to enhance the visiility ex in ecg graphs
plt.show()
